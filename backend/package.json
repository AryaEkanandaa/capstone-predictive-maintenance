{
  "name": "predictive-maintenance-backend",
  "version": "1.0.0",
  "description": "Backend for Predictive Maintenance Capstone",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node-pg-migrate"
  },
  "node-pg-migrate": {
    "databaseUrl": {
      "ENV": "DATABASE_URL"
    },
    "dir": "migrations",
    "migrationsTable": "pgmigrations",
    "checkOrder": false
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^1.1.4",
    "@langchain/google-genai": "^2.0.4",
    "@langchain/groq": "^1.0.2",
    "@langchain/langgraph": "^1.0.4",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^1.1.5",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "node-pg-migrate": "^7.6.1",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
